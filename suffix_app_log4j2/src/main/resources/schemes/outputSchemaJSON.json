{
  "$schema": "http://json-schema.org/draft-07/schema#",

  "title": "Root",
  "type": "object",
  "properties": {

    "suffixingApp": {
      "title": "SuffixingApp",
      "type": "array",

      "items":{
        "title": "Items",
        "type": "object",

        "properties": {

          "oldName": {
            "title": "oldName",
            "type": "string"
          },

          "newName": {
            "title": "newName",
            "type": "string"
          },

          "dateTime": {
            "title": "dateTime",
            "type": "string",
            "pattern": "\\d{4}-\\d{2}-\\d{2}\\s\\d{2}:\\d{2}:\\d{2}\\.\\d+"
          },

          "configFileName": {
            "title": "configFileName",
            "type": "string"
          }
        }
      }

    }
  }
}
